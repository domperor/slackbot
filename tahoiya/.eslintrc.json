{
    "extends": "@hakatashi/eslint-config/typescript",
    "settings": {
        "import/resolver": {
            "typescript": {
                "directory": "./tsconfig.json"
            }
        }
    },
    "rules": {
        "no-console": "off",
        "no-div-regex": "off",
        "no-nested-ternary": "off",
        "no-unused-vars": "off",
        "sort-imports": "off",
        "no-restricted-imports": "off",
        "react/no-access-state-in-setstate": "off",
        "import/extensions": "off"
    }
}
