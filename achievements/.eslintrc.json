{
    "extends": "@hakatashi/eslint-config/typescript",
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-imports": "off",
        "no-use-before-define": "off",
        "camelcase": "off",
        "import/extensions": "off"
    }
}
