{
    "extends": "@hakatashi",
    "parserOptions": {
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "require-atomic-updates": "off"
    }
}
